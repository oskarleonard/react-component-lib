{
  "engines": {
    "node": "10.*"
  },
  "name": "@oskarleonard/react-component-lib",
  "description": "React starter kit",
  "version": "3.0.0",
  "main": "src/server/server.js",
  "license": "MIT",
  "scripts": {
    "deploy": "npm publish",
    "prod": "NODE_ENV=production concurrently --kill-others \"node _build_prod/bin/server.prod.js\"",
    "dev": "rimraf _build_dev && cross-env NODE_ENV=development concurrently --kill-others \"node bin/server.dev.js\"",
    "buildscript": "cross-env NODE_ENV=production node scripts/build",
    "bundle-analyzer": "webpack-bundle-analyzer _build_prod/client/stats.json",
    "preparecommitmsg": "node scripts/hooks/prepare-commit-msg",
    "preparecommitmsg:doc": "echo 'workaround for windows users'",
    "test": "jest --config jest.config.json --coverage",
    "test:watch": "jest --watch --config jest.config.json",
    "test:update": "jest --config jest.config.json --updateSnapshot",
    "fix": "prettier-eslint --write \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "common-tags": "1.8.0",
    "compression": "1.7.4",
    "concurrently": "5.0.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "extract-css-chunks-webpack-plugin": "^3.2.1",
    "history": "4.10.1",
    "morgan": "1.9.1",
    "prop-types": "15.7.2",
    "react": "16.11.0",
    "react-cookie": "4.0.1",
    "react-cropper": "^1.3.0",
    "react-dom": "16.11.0",
    "react-dropzone": "^10.1.10",
    "react-helmet-async": "^1.0.4",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "react-universal-component": "^4.0.0",
    "serve-favicon": "2.5.0",
    "shelljs": "0.8.3",
    "universal-cookie-express": "4.0.1",
    "uuid": "3.3.3",
    "webpack-flush-chunks": "2.0.3",
    "zlib": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.3",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@hot-loader/react-dom": "^16.10.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-universal-import": "^3.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.12.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "react-hot-loader": "^4.12.10",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:oskarleonard/react-component-lib.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@oskarleonard"
  }
}
